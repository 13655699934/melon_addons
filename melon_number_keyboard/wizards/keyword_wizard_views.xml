<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="virtual_keyword_wizard_form" model="ir.ui.view">
            <field name="name">virtual_keyword_wizard_form</field>
            <field name="model">virtual.keyword.wizard</field>
            <field name="arch" type="xml">
                <form string="">
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="virtual_float" widget="float_number_keyboard"/>
                        </group>
                        <group>
                            <field name="virtual_integer" widget="integer_number_keyboard"/>
                        </group>

                    </group>
                    <footer>
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

    </data>
</odoo>
