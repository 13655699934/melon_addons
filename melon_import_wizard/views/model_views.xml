<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_melon_data_order_tree" model="ir.ui.view">
        <field name="name">订单管理</field>
        <field name="model">melon.data.order</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="code"/>
                <field name="order_name"/>
                <field name="ded_expenses"/>
                <field name="close_amount"/>
                <field name="notes"/>
            </tree>
        </field>
    </record>

    <record id="view_melon_data_order_form" model="ir.ui.view">
        <field name="name">订单管理</field>
        <field name="model">melon.data.order</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="code"/>
                            <field name="order_name"/>
                            <field name="ded_expenses"/>
                            <field name="close_amount"/>
                            <field name="notes"/>
                        </group>
                        <group>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>


</odoo>

