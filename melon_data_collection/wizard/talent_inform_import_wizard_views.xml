<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="base_talent_info_wizardform" model="ir.ui.view">
            <field name="name">import.talent.info.wizard.form</field>
            <field name="model">import.talent.info.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <group>
                            <field name="data" filename="data_filename" required="1"/>
                            <field name="data_filename" invisible="1"/>
                        </group>
                    </group>
                    <footer>
                        <button name="btn_confirm" string="确认" type="object" class="oe_highlight"/>
                        <button special="cancel" string="取消" class="oe_link"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>